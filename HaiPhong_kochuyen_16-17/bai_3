#include<bits/stdc++.h>
#define ll long long
#define fast ios_base::sync_with_stdio(false);cin.tie(nullptr);cout.tie(nullptr)
using namespace std;
const int N=1e5+5;
int a[N],b[N];
int n,m;
int main () {
    freopen("BAI3.INP","r",stdin);
    freopen("BAI3.OUT","w",stdout);
    fast;
    cin>>n>>m;
    map<int,int> mp;
    for(int i=0;i<n;++i) {
        cin>>a[i];
        mp[a[i]%3]++;
    }
    int res[m];
    for(int i=0;i<m;++i) {
        cin>>b[i];
        if (b[i]%3==1) res[i]=mp[2];
        else if (b[i]%3==2) res[i]=mp[1];
        else res[i]=mp[0];
    }
    for(int i=0;i<m;++i)
        cout<<res[i]<<endl;

}

